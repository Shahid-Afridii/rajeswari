{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "nodeVersion": "16.x"
        }
      }
    ],
    "outputDirectory": "build",
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ]
  }
  